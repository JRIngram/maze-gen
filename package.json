    "@types/jest": "^29.5.14",
    "eslint": "^9.20.1",
    "globals": "^16.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
