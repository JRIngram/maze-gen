  },
  "keywords": [
    "maze",
    "generator",
    "maze-gen",
    "maze-gen",
    "gen",
    "depth-first",
    "hunt-and-kill",
    "huntandkill",
    "procedural",
    "recursive",
    "backtracker",
    "depth",
    "first",
    "depth-first",
    "seed",
    "reproducible"
  ],
  "main": "index.js",
  "scripts": {
